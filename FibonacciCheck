def is_fibonacci(num):
    """Checar se o numero pertence à sequência de Fibonacci."""
    # Sequencia inicial Fibonacci 
    a, b = 0, 1
    
    while a <= num:
        if a == num:
            return True
        a, b = b, a + b
    return False

# Input: Pegue o número do usuário
try:
    number = int(input("Digite um número para verificar se pertence à sequência de Fibonacci: "))
    
    # Checar se o número pertence a sequencia Fibonacci
    if is_fibonacci(number):
        print(f"O número {number} pertence à sequência de Fibonacci.")
    else:
        print(f"O número {number} NÃO pertence à sequência de Fibonacci.")
except ValueError:
    print("Por favor, insira um número inteiro válido.")

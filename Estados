# Dados de faturamento por estado
faturamento_estados = {
    "SP": 67836.43,
    "RJ": 36678.66,
    "MG": 29229.88,
    "ES": 27165.48,
    "Outros": 19849.53
}

# Função para calcular o percentual de cada estado
def calcular_percentuais(faturamento_estados):
    """Calcula e exibe o percentual de faturamento de cada estado."""
    # Calcular o total de faturamento
    total_faturamento = sum(faturamento_estados.values())
    
    # Calcular e exibir o percentual de cada estado
    for estado, faturamento in faturamento_estados.items():
        percentual = (faturamento / total_faturamento) * 100
        print(f"Percentual de {estado}: {percentual:.2f}%")

# Chamada da função
if __name__ == "__main__":
    calcular_percentuais(faturamento_estados)
